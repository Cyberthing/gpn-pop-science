{
  "name": "<%PROJECT_NAME%>",
  "title": "<%PROJECT_NAME%>",
  "description": "",
  "devPort": "<%DEV_PORT%>",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "webpack" : "webpack",
    "start": "echo starting at localhost:<%DEV_PORT%> && webpack-dev-server --port=<%DEV_PORT%> --env.entry=dev",
    "build-critical": "webpack --mode production --env.build=production --env.entry=critical",
    "build": "webpack --mode production --env.build=production",
    "build-dev": "webpack --mode development --env.build=development",
    "build-en": "webpack --mode production --env.build=production --env.locale=en",
    "analyze": "webpack --mode production --env.build=production --env.analyze=1",
    "critical": "react-snap"
  },
  "reactSnap": {
    "source": "build",
    "destination": "critical",
    "include": [
      "/critical.html"
    ],
    "viewport": {
      "width": 1440,
      "height": 900
    },
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    }
  },
  "reactSnapshot": {
    "include": [
      "/critical.html"
    ],
    "exclude": [
      "/"
    ],
    "snapshotDelay": 300
  },
